public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> mapResult=new Map<String, List<Case>>();
    List<Case> listCases=new List<Case>();
    for(Case c:cases){

        if(c.Type!=null){
            if(!mapResult.containsKey(c.Type)){
            mapResult.putAll(c.Type,listCases.add(c));
            }
            else{
              List<Case> caseList= mapResult.get(c.Type);
              caseList.add(c);
              mapResult.putAll(c.Type,caseList);
            }

        }
    }
    return mapResult;
}