public Integer maxProfit(List<Integer> prices){
    //code here
    Integer maxPrice=0;
        Integer minPrice=prices[0];
        Integer dayminIndex=0;
        Integer daymaxIndex=0;
        for(Integer i=0;i<prices.size();i++){
            
            if(maxPrice<=prices[i]){
                maxPrice=prices[i];//10
                daymaxIndex=i;//0
            } else if(minPrice>=prices[i]){
                minPrice=prices[i]; //2
                dayminIndex=i;//1
            }
            if(dayminIndex>daymaxIndex){maxPrice=minPrice;}
        }
        if(dayminIndex<daymaxIndex){
            return maxPrice-minPrice;
        }
        else{
            return 0;
        }
        
}