public void defragging(Integer[] memory)
{
    // code here

Integer left = 0;


  Integer nextPos = memory.size()-1 ;;
    for (Integer i = 0; i < memory.size(); i++) {
        if (memory[i] != -1) {
            Integer temp = memory[nextPos];
            memory[i]= memory[i+1];
            memory[nextPos]=-1;
            nextPos++;
        }
    }
}