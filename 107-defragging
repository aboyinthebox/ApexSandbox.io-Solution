public void defragging(Integer[] memory)
{
    // code here

Integer left = 0;
Integer right = memory.size() - 1;

while (left < right) {//0-
    if (memory[left] == -1) {
        if (memory[right] != -1) {//3
            for(Integer i=0;i<memory.size();i++){
                memory[i]=memory[i+1];
            }
            memory[right]=-1;
            left++;
            right--;
        } else {
            right--;
        }
    } else {
        left++;
    }
}

}