public Boolean isSubSequence(String s1, String s2){
    //code here
    Integer pointer=0;
    for(Integer i=0;i<s1.length();i++){
          Boolean isMatchFound=false;
          for(Integer j=0;j<s2.length();j++){
              if(s1.charAt(i)==s2.charAt(j)){
                  //tworraord
                  isMatchFound=true;
                   if(j>=pointer) //1
                    {
                        pointer=j;
           
                    }
              }

          }
           if(!isMatchFound) return false;
    }
    return false;
}