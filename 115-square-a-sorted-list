public static List<Integer> squareList(List<Integer> nums){
    Integer left=0,right=nums.size()-1;
        List<Integer> result=new List<Integer>();
        Integer lastIndex=nums.size()-1;

        while(left<=right){
            Integer leftSquare=nums[left];
            Integer rightSquare=nums[right];
            
            if(leftSquare>rightSquare){
                result[lastIndex--]=leftSquare;
                left++;
            } else{
                result[lastIndex--]=rightSquare;
                right--;
            }
            
            
        }     
    return result;
}