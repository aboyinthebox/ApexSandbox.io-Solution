public Id classFromCourse(String courseName) {
    // code here
    List<apxio__Course__c> courses = [ SELECT id FROM apxio__Course__c WHERE Name =:courseName ];
    if(courses.size()>0 ){
        apxio__Class__c newClass=new apxio__Class__c(apxio__Course__c=courses[0].Id);
        insert newClass;
        return newClass.Id;
    }
    else{
        return null;
    }
    
}