public Id classFromCourse(String courseName) {
    // code here
    apxio__Course__c course = [ SELECT id FROM apxio__Course__c WHERE Name =:courseName ];
    if(course!= null){
        apxio__Class__c newClass=new apxio__Class__c(apxio__Course__c=courseName);
        insert newClass;
        return newClass.Id;
    }
    else{
        return null;
    }
    
}