public string countAndSay(integer N) {

   String currentSequence = '';
        for (Integer i = 1; i < N; i++) {
            currentSequence = countAndSayHelper(currentSequence);
        }
   return currentSequence;

}
public string countAndSayHelper(String res){
 
    if(String.isEmpty(res)){
         return '1';
     }
        String newSequence = '';
        Integer count = 1;
        Integer res = res.length();
        List<String> sp = res.split('');
        
          while (i < n){

        Integer j = i + 1;

        Integer count = 1;

        // COUNT DUPLICATE

        while (j < n && sp.get(j - 1) == sp.get(j)){

            count++;

            j++;

        }

        if (i < n){

            newSequence += count + '' + sp.get(i);

        }

        i = j;

    }

    return newSequence;

}